{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/api/wa/(.*)",
      "destination": "https://pixelup-wa-server.pixelupmrkt.repl.co/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/wa/qr(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" }
      ]
    }
  ]
}
